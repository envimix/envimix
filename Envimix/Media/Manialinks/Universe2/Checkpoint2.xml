			</frame>
			<label pos="-4 -0.2" z-index="0" size="25 6" text="01" valign="center2" textsize="1" textfont="RajdhaniMono" halign="center"/>
		</frame>
		<frame id="FrameRecordType" pos="5 0" clip="1" clipsizen="60 6">
			<frame id="FrameBackground" z-index="-1">
				<quad z-index="0" size="30 6" valign="center" halign="right" style="Bgs1" substyle="BgCardList"/>
			</frame>
			<label pos="-15 -0.2" z-index="0" size="25 6" textprefix="$t" text="Personal Record" valign="center2" textsize="1" textfont="RajdhaniMono" halign="center" translate="1"/>
