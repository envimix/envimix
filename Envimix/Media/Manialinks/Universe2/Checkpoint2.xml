			<frame id="FrameBackground" z-index="-1">
				<quad z-index="0" size="12 7" valign="center" halign="left" style="Bgs1" substyle="BgCardList"/>
			</frame>
			<label pos="6 -0.2" z-index="0" size="10 6" textprefix="/ " text="01" valign="center2" textsize="1.75" textfont="RajdhaniMono" halign="center"/>
		</frame>
		<frame id="FrameDifferences" pos="0 42">
			<frameinstance modelid="FramemodelDifference"/>
			<frameinstance pos="0 -6" modelid="FramemodelDifference"/>
			<frameinstance pos="0 -12" modelid="FramemodelDifference"/>
			<frameinstance pos="0 -18" modelid="FramemodelDifference"/>
			<frameinstance pos="0 -24" modelid="FramemodelDifference"/>
			<frameinstance pos="0 -30" modelid="FramemodelDifference"/>
		</frame>
