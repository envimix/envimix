		</frame>
	</framemodel>
	<frame id="FrameCheckpoint" scale="0.85">
		<frame id="FrameLap" pos="0 58" clip="1" clipsizen="40 7">
			<frame id="FrameBackground" z-index="-1">
				<quad z-index="0" size="20 6" valign="center" halign="center" style="Bgs1" substyle="BgCardList"/>
				<quad id="QuadColor" z-index="2" size="20 6" valign="center" halign="center" style="Bgs1" substyle="BgWindow4" modulatecolor="DA0" opacity=".5"/>
			</frame>
			<label id="LabelLapTime" pos="0 -0.2" z-index="0" size="20 6" text="0:00.000" valign="center2" textsize="1.3" textfont="RajdhaniMono" halign="center"/>
		</frame>
		<frame id="FrameCheckpointRank" pos="-20 49" hidden="1">
			<frame id="FrameBackground" z-index="-1">
				<quad z-index="0" size="12 7" valign="center" halign="right" style="Bgs1" substyle="BgCardList"/>
			</frame>
			<label pos="-6 -0.2" z-index="0" size="10 6" text="01" valign="center2" textsize="1.75" textfont="RajdhaniMono" halign="center"/>
		</frame>
		<frame id="FrameCheckpointTime" pos="0 50">
			<frame id="FrameBackground" z-index="-1">
				<quad z-index="0" size="40 9" valign="center" halign="center" style="Bgs1" substyle="BgCardList"/>
			</frame>
			<frame id="FrameInnerCheckpointTime" clip="1" clipsizen="40 6">
